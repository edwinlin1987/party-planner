{
  "name": "party-planner",
  "version": "0.0.1",
  "description": "A party planning web application",
  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "check-coverage": "istanbul check-coverage --statements 0 --branches 0 --functions 0 --lines 0",
    "validate": "npm-run-all --parallel validate-webpack:* lint test --serial check-coverage",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "start": "node server/main",
    "prebuild": "npm run clean-dist",
    "prebuild:prod": "npm run clean-dist",
    "build": "webpack --env.dev",
    "build:prod": "webpack --env.prod -p",
    "clean-dist": "rimraf dist",
    "prestart": "npm run clean-dist",
    "lint": "eslint ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edwinlin1987/party-planner.git"
  },
  "author": "Edwin Lin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/edwinlin1987/party-planner/issues"
  },
  "homepage": "https://github.com/edwinlin1987/party-planner#readme",
  "dependencies": {
    "connect-history-api-fallback": "^1.3.0",
    "express": "^4.14.0",
    "lodash": "^4.16.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-plugin-react": "^1.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^3.5.0",
    "cpy-cli": "^1.0.1",
    "debug": "^2.2.0",
    "eslint": "^3.6.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-config-standard-react": "^4.0.2",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.3.0",
    "eslint-plugin-standard": "^2.0.0",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "^0.4.5",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "npm-run-all": "^3.1.0",
    "rimraf": "^2.5.4",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.3",
    "webpack-hot-middleware": "^2.12.2"
  }
}
